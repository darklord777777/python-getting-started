def log(message, answer):
    print("/n ------")
    from datetime import datetime
    print(datetime.now())
    print("–°–æ–æ–±—â–µ–Ω–∏–µ –æ—Ç {0} {1}. (id = {2}) \n –¢–µ–∫—Å—Ç = {3}".format(message.from_user.first_name, message.from_user.last_name, str(message.from_user.id), message.text))

kk = '–ö–µ–∫'
ll = '–õ–æ–ª'
kl = '–ö–µ–∫–õ–æ–ª'
lk = '–õ–æ–ª–ö–µ–∫'
klk = '–ö–µ–∫–ª–æ–ª–ö–µ–∫'
kkc = '1000'
llc = '1000'
klc = '1000'
lkc = '1000'
klkc = '1000'
qw = '79142371237'
cl = '–û—Ç–º–µ–Ω–∏—Ç—å –∑–∞–∫–∞–∑'


@bot.message_handler(commands=['start'])
def handle_text(message):
        answer = '–ü—Ä–∏–≤–µ—Ç—Å—Ç–≤—É—é ‚úãüèª! –ù–∞–∂–º–∏—Ç–µ "–ì–æ—Ä–æ–¥" , —á—Ç–æ–±—ã –≤—ã–±—Ä–∞—Ç—å –≤–∞—à –≥–æ—Ä–æ–¥ üè¢ –∏ —Å–¥–µ–ª–∞—Ç—å –∑–∞–∫–∞–∑ üéÅ. –ù–∞–∂–º–∏—Ç–µ "–ú–µ–Ω—é" , —á—Ç–æ–±—ã –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å —Ü–µ–Ω—ã üí≤ –∏ –Ω–∞–ª–∏—á–∏–µ —Ç–æ–≤–∞—Ä–∞ üéÅ. –¢–∞–∫–∂–µ —Ç—Ä–µ–±—É—é—Ç—Å—è —Ä–∞–±–æ—Ç–Ω–∏–∫–∏ üì¨üñç —Å –≤—ã—Å–æ–∫–æ–π –∑–∞—Ä–æ–±–æ—Ç–Ω–æ–π –ø–ª–∞—Ç–æ–πüí∞üí∞üí∞, –¥–ª—è —Ä–∞–±–æ—Ç—ã –Ω–µ–æ–±—Ö–æ–¥–∏–º –∑–∞–ª–æ–≥, –ø–æ–¥—Ä–æ–±–Ω–æ—Å—Ç–∏ –≤ —Ç–µ—Ö.–ø–æ–¥–¥–µ—Ä–∂–∫—É. –¢–µ—Ö.–ø–æ–¥–¥–µ—Ä–∂–∫–∞: @KekLolSupp.'
        log(message, answer)
        markup = types.ReplyKeyboardMarkup(row_width=1)
        itembtn1 = types.KeyboardButton('–ì–æ—Ä–æ–¥')
        itembtn2 = types.KeyboardButton('–ú–µ–Ω—é')
        itembtn3 = types.KeyboardButton('–°—Ç–æ–ø')
        markup.add(itembtn1, itembtn2, itembtn3)
        bot.send_message(message.from_user.id, answer, reply_markup=markup)


@bot.message_handler(content_types=['text'])
def handle_text(message):
 if message.text == "–ì–æ—Ä–æ–¥":
    answer = '–í—ã–±–µ—Ä–µ—Ç–µ –≤–∞—à –≥–æ—Ä–æ–¥ üè¢'
    log(message, answer)
    markup = types.ReplyKeyboardMarkup(row_width=1)
    itembtn1 = types.KeyboardButton('–í–ª–∞–¥–∏–≤–æ—Å—Ç–æ–∫')
    itembtn2 = types.KeyboardButton('–ê—Ä—Ç–µ–º')
    itembtn3 = types.KeyboardButton('–£—Å—Å—É—Ä–∏–π—Å–∫')
    itembtn4 = types.KeyboardButton('–ù–∞—Ö–æ–¥–∫–∞')
    itembtn5 = types.KeyboardButton('–ê—Ä—Å–µ–Ω—å–µ–≤')
    itembtn6 = types.KeyboardButton('–ù–∞–∑–∞–¥')
    itembtn7 = types.KeyboardButton('–°—Ç–æ–ø')
    markup.add(itembtn1, itembtn2, itembtn3, itembtn4, itembtn5, itembtn6, itembtn7)
    bot.send_message(message.from_user.id, answer, reply_markup=markup)

 elif message.text == "–í–ª–∞–¥–∏–≤–æ—Å—Ç–æ–∫":
       ans = '–í—ã–±–µ—Ä–µ—Ç–µ –≤–∞—à –º–∏–∫—Ä–æ—Ä–∞–π–æ–Ω üè¢'
