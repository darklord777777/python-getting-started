    itembtn2 = types.KeyboardButton('–û—Ç–º–µ–Ω–∏—Ç—å –∑–∞–∫–∞–∑')
    answer = "–û–ñ–ò–î–ê–ï–ú –û–ü–õ–ê–¢–´ üí≤üí≤üí≤"
    log(message, answer)
    markup.add(itembtn1, itembtn2)
    bot.send_message(message.from_user.id, answer, reply_markup=markup)

 elif message.text == "–û—Ç–º–µ–Ω–∏—Ç—å –∑–∞–∫–∞–∑":
   markup = types.ReplyKeyboardRemove()
   answer = "–í—ã –∑–∞–±–∞–Ω–µ–Ω—ã –Ω–∞ 5 –º–∏–Ω—É—Ç, –∑–∞–ø—Ä–µ—â–µ–Ω–æ —Ä–µ–∑–µ—Ä–≤–∏—Ä–æ–≤–∞—Ç—å –∑–∞–∫–∞–∑ –±–µ–∑ –æ–ø–ª–∞—Ç—ã‚ùóÔ∏è‚ùóÔ∏è‚ùóÔ∏è"
   log(message, answer)
   bot.send_message(message.from_user.id, answer, reply_markup=markup)

 elif message.text == "–°—Ç–æ–ø":
    answer = "–î–æ —Å–∫–æ—Ä–æ–π –≤—Å—Ç—Ä–µ—á–∏!ü§òüèª"
    log(message, answer)
    markup = types.ReplyKeyboardRemove()
    bot.send_message(message.from_user.id, answer, reply_markup=markup)

 else:

     ans = '–ù–∞–∂–º–∏—Ç–µ "–ì–æ—Ä–æ–¥" , —á—Ç–æ–±—ã –≤—ã–±—Ä–∞—Ç—å –≤–∞—à –≥–æ—Ä–æ–¥ üè¢ –∏ —Å–¥–µ–ª–∞—Ç—å –∑–∞–∫–∞–∑ üéÅ. –ù–∞–∂–º–∏—Ç–µ "–ú–µ–Ω—é" , —á—Ç–æ–±—ã –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å —Ü–µ–Ω—ã üí≤ –∏ –Ω–∞–ª–∏—á–∏–µ —Ç–æ–≤–∞—Ä–∞ üéÅ. –¢–∞–∫–∂–µ —Ç—Ä–µ–±—É—é—Ç—Å—è —Ä–∞–±–æ—Ç–Ω–∏–∫–∏ üì¨üñç —Å –≤—ã—Å–æ–∫–æ–π –∑–∞—Ä–æ–±–æ—Ç–Ω–æ–π –ø–ª–∞—Ç–æ–πüí∞üí∞üí∞, –¥–ª—è —Ä–∞–±–æ—Ç—ã –Ω–µ–æ–±—Ö–æ–¥–∏–º –∑–∞–ª–æ–≥, –ø–æ–¥—Ä–æ–±–Ω–æ—Å—Ç–∏ –≤ —Ç–µ—Ö.–ø–æ–¥–¥–µ—Ä–∂–∫—É. –¢–µ—Ö.–ø–æ–¥–¥–µ—Ä–∂–∫–∞: @KekLolSupp.'
     log(message, ans)
     markup = types.ReplyKeyboardMarkup(row_width=1)
     itembtn1 = types.KeyboardButton('–ì–æ—Ä–æ–¥')
     itembtn2 = types.KeyboardButton('–ú–µ–Ω—é')
     itembtn3 = types.KeyboardButton('–°—Ç–æ–ø')
     markup.add(itembtn1, itembtn2, itembtn3)
     bot.send_message(message.from_user.id, ans, reply_markup=markup)
bot.polling(none_stop=True)
